        <activity android:name=".ui.tag.KeywordTagActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.group.GroupListActivity"/>
        <activity android:name=".ui.signup.SignuplastActivity"></activity>
        <activity android:name=".ui.signup.SignupSecondActivity" />
        <activity android:name=".ui.signup.SignupActivity" />
