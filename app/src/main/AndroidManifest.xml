        <activity android:name=".ui.group.EndGroupJoinActivity" />
        <activity android:name=".ui.group.SignUpGroupActivity" />
        <activity android:name=".ui.group.JoinGroupActivity" />
        <activity android:name=".ui.group.WaitGroupActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.login.LoginPageActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.upload.UploadPlaceActivity" />
        <activity android:name=".ui.tag.UsefulTagActivity"/>
        <activity android:name=".ui.search.place.PlaceSearchActivity" />
